<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<import resource="dataSourceConfig.xml" />
	<bean id="InsertBatch"
		class="com.yinzifan.liandisys._0920_Spring06_SpringJdbcTemplateBatchUpdate.InsertBatch">
		<property name="dataSource" ref="dataSource" />
	</bean>
	<bean id="QueryBiz"
		class="com.yinzifan.liandisys._0920_Spring06_SpringJdbcTemplateBatchUpdate.QueryBiz">
		<property name="dataSource" ref="dataSource" />
	</bean>
</beans>